[user]
	name = powerbombkun
	email = powerbombkun@gmail.com
[alias]
        st = status
        ci = commit -a
        co = checkout
        br = branch
        up = pull --rebase
        graph = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
        log-all = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
[color]
        branch = auto
        diff = auto
        interactive = auto
        status = auto
